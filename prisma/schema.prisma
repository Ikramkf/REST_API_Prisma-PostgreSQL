// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") //avoid committed secrets 
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  firstName String
  lastName String
  social Json?
}

model Course{
  id Int @id @default(autoincrement())

}